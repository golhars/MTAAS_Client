<script>

    var app=angular.module('MtaaS', []);
    app.controller('MtaaS1', ['$scope', '$http', '$window', function ( $scope,$http,$window)
    {
       // alert("i am inside controller");



        $scope.testedapp1 = function() {
            $http({
                method: 'GET',
                url: '/testedapp1'
            }).success(function(response){
               // alert("testedapp: **********************************" + JSON.stringify(response));
                $scope.testedapp= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.testedapp1();

$scope.testerinfo2=function(){
    $http({
        method: 'GET',
        url: '/testerinfo2'
    }).success(function(response){
    //    alert("testinfo ******************************" + JSON.stringify(response));
        $scope.testinfo= JSON.parse(response.result);
        //alert(processes.suppliername);
    }).error(function(error){
        alert("ERROR VIEWING PROCESS REQUEST");
    });

}
$scope.testerinfo2();


        $scope.currentapp_customer = function() {
            $http({
                method: 'GET',
                url: '/currentapp_customer'
            }).success(function(response){
              //  alert("curfrentapp customer****************************************************************" + JSON.stringify(response));
                $scope.currentapp_customer= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.currentapp_customer();


        $scope.customerinfo2 = function() {
     //       alert("I am inside testerinfo");

            $http({
                method: 'GET',
                url: '/customerinfo2'
            }).success(function(response){
             //   alert(JSON.stringify(response));
                $scope.customer= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.customerinfo2();

    }]);
</script>

