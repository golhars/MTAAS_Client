                                                    <th>App Name</th>
                                                    <th>Status</th>

                                                </tr>
                                                </thead>
                                                <tbody>
                                                <tr ng-repeat="c in currentapp_customer">

                                                    <td>{{c.app_name}}</td>
                                                    <td>{{c.status}}</td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div>





                                    <div class="panel panel-primary">
                                        <div class="panel-heading">
                                            <h4 class="text-center">Pay: </h4>
                                        </div>


                                        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                            <thead>
                                            <tr class="success">

                                                <th>Amount Paid</th>
                                                <th>App Name</th>

                                            </tr>
                                            </thead>
                                            <tbody>
                                            <tr ng-repeat="t in testedapp">

                                                <td><a href="/customer_pay" target="_blank">${{t.estimated_cost}}</a></td>
                                                <td>{{t.app_name}}</td>
                                            </tr>
                                            </tbody>
                                        </table>

                                    </div>

                                    <div class="panel panel-primary">
                                        <div class="panel-heading">
