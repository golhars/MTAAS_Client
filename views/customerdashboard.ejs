<div class="container" data-ng-app="MtaaS" data-ng-controller="MtaaS1">

    <div class="row">
        <div class="box">
            <div class="col-lg-28 text-center">
                <div id="carousel-example-generic" class="carousel slide">


                    <!-- Wrapper for slides -->
                    <div class="carousel-inner">
                        <div class="container" ng-repeat="t in customer">
                            <div class="row">
                                <div class="col-sm-3" >
                                    <div class="panel panel-primary" >
                                        <div class="panel-heading">
                                            <b><a href=/customer_addapp style="color: #ffffff" class="text-center" font-size="9"  >Add Application<span class="glyphicon glyphicon-user pull-right"></span></a></b>
                                        </div>
                                        <div class="panel-body text-center">
                                            <p class="lead">
                                                <strong>{{t.username}}</strong>
                                            </p>
                                        </div>
                                        <!--ul class="list-group list-group-flush">
                                            <li class="list-group-item liitem"><strong>No. Of apps : </strong>
                                                <span class="pull-right">{{t.}}</span>
                                            </li>
                                            <li class="list-group-item liitem"><strong>Testing Technology :</strong>
                                                <span class="pull-right">{{t.testing_technology}}</span>
                                            </li>
