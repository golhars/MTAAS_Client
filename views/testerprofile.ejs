<script>
    var app=angular.module('MtaaS', [])
    app.controller('MtaaS', ['$scope', '$http', '$window', function ( $scope,$http,$window)
    {
     //   alert("I am in controller");
        var in_name;
        var in_phone;
        var in_email;
        var in_fname;
        var in_mname;
        var in_lname;
        var in_address1;
        var in_address2;
        var in_country;
        var in_zip;
        var in_linkedin;
        var in_password;
        var in_cpassword;

        var in_experience;
        var in_functional;
        var in_unit;
        var in_testing;
        var in_tool;
        var in_language;
        //Tester Sign up function

        $scope.signup = function () {
     //       alert("I am in signup function");
            in_name = $scope.name;
      //      alert(in_name);
            in_phone = $scope.phone;
            in_email = $scope.email;
            in_fname = $scope.fname;
            in_mname = $scope.mname;
            in_lname = $scope.lname;
            in_address1 = $scope.address1;
            in_address2 = $scope.address2;
            in_country = $scope.country;
            in_zip = $scope.zip;
            in_linkedin = $scope.linkedin;
            in_password = $scope.password;
            in_cpassword = $scope.cpassword;

            //console.log("doing the validation here ");

//Validation for fields not empty

            if (in_fname == null || in_fname == "") {
                alert("Enter first name!!");
                return;
            }

            if (in_lname == null || in_lname == "") {
                alert("Enter last name");
                return;
            }

            if (in_email == null || in_email == "") {
                alert("Enter Email");
                return;
            }
            if (in_name == null || in_name == "") {
                alert("Enter Username");
                return;
            }

            if (in_password == null || in_password == "") {
                alert("Enter a Password");
                return;
            }
            //validation for password and confirm password
            if (in_password != in_cpassword) {
                alert("Passwords don't match.");
                return;
            }
        };
        /*
         $http({
         method: 'POST',
         url: '/signup',
         data: {"fname":in_fname,"mname": in_mname, "lname":in_lname,"email": in_email,"name":in_name,"password":in_password, "address1":in_address1,"address2":in_address2,"country":in_country,"zip": in_zip,"linkedin": in_linkedin,"phone": in_phone}
         }).
         then(function(response) {

         $window.location.href = '/index';
         });
         }
         */

        //Testing function
        $scope.testing = {
            functional: '',
            unit : '',
            performance : '',
            usability : '',
            security : '',
            toolA : '',
            toolB : '',
            toolC : '',
            english: '',
            french: '',
            japanese: ''
        };

        $scope.testerdetail = function () {
     //       alert("I am in testerdetail");
            in_experience= $scope.experience;
            in_functional = $scope.testing.functional;
            in_unit = $scope.testing.unit;
            in_testing = in_functional.concat(in_unit, $scope.testing.performance, $scope.testing.usability, $scope.testing.security);
            in_tool = $scope.testing.toolA.concat($scope.testing.toolB, $scope.testing.toolC);
            in_language = $scope.testing.english.concat($scope.testing.french, $scope.testing.spanish);
      //      alert(in_language);

            if (in_experience == null || in_experience == "") {
                in_experience = 0;
                return;
            }
            /*
             $http({
             method: 'POST',
             url: '/testerdetail',
             data: {"experience": in_experience, "testingtype": in_testing, "testingtool": in_tool, "language": in_language}
             }).
             then(function (response) {
             $window.location.href = '/index';
             });


             alert("i am here after http");
             //alert(in_testing);
             //alert($scope.testing.unit);

             //alert(in_testing);
             */
        }


    }

    $scope.home = function (){
        $window.location.href = '/home';
    }
    $scope.logoutsession = function ()
    {
        $window.location.href = '/logout';
    }

    $scope.about=function()
    {
        $window.location.href='/about';
    }
    $scope.blog=function()
    {
        $window.location.href='/blog';
    }
    $scope.contact=function()
    {
        $window.location.href='/contact';
    }




    ]);
</script>


