<script>

    var app=angular.module('MtaaS', ['chart.js']);
    app.controller('MtaaS1', ['$scope', '$http', '$window', function ( $scope,$http,$window)
    {
     //   alert("i am inside controller");



        $scope.testedapp = function() {
            $http({
                method: 'GET',
                url: '/testedapp'
            }).success(function(response){
        //        alert("testedapp: **********************************" + JSON.stringify(response));
                $scope.testedapp= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.testedapp();


        $scope.appinfo = function() {
            $http({
                method: 'GET',
                url: '/appinfo'
            }).success(function(response){
      //          alert("Appinfo ******************************" + JSON.stringify(response));
                $scope.appinfo= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.appinfo();


        $scope.currentapp = function() {
            $http({
                method: 'GET',
                url: '/currentapp'
            }).success(function(response){
    //            alert(JSON.stringify(response));
                $scope.currentapp= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.currentapp();


        $scope.testerinfo = function() {
    //    alert("I am inside testerinfo");

            $http({
                method: 'GET',
                url: '/testerinfo',
            }).success(function(response){
         //       alert(JSON.stringify(response));
                $scope.tester= JSON.parse(response.result);
                //alert(processes.suppliername);
            }).error(function(error){
                alert("ERROR VIEWING PROCESS REQUEST");
            });
        };
        $scope.testerinfo();

        $scope.myoptions = {
            pointLabelFontSize: 26,
        };

        $scope.labels =["Total Test Cases", "Test Cases Pending", "Test Cases Completed", " Bug's found"];

        $scope.data = [
            //[65, 59, 90, 81, 56, 55, 40],
            [150, 15,60 , 5]
        ];


    }]);
</script>


